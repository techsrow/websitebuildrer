@model WebsiteBuilder.Models.Couter
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@using Microsoft.AspNetCore.Identity
@{
    ViewData["Title"] = "Create";

    Layout = "~/Views/Shared/_WebsiteCreateLayout.cshtml";

}
<form asp-action="Create" method="post" enctype="multipart/form-data">
    @Html.AntiForgeryToken()

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <label>Title</label>
    <input asp-for="Title" class="form-control" />

    <label>Starting Number</label>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input asp-for="StartNumber" class="form-control" />


    <label>Ending Number</label>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input asp-for="EndNumber" class="form-control" />





   

    <input type="submit" value="Create" style="background-color: #FEB800;border:none;" class="btn btn-primary btn-block btn-lg" />
</form>